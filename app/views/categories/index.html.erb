<h1 align="center">Listing All Categories</h1>
<div class="container-fluid">
  <div class="row">
    <nav class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
      <ul class="list-group">
        <li class="list-group-item">
          <h1 align="center">Popular categories
          </h1>
        </li>
        <li class="list-group-item">
        </li>
        <li class="list-group-item">
          <span class="badge">257</span>
          Photograpy
        </li>
        <li class="list-group-item">
          <span class="badge">324</span>
          Nightlife
        </li>
      </ul>
    </nav>
    <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9 ">
      <div align="center">
        <%= will_paginate %></div>
          <ul class="listing">
            <div class="container-fluid">
              <div class="row">
                <% @categories.each do |category| %>
                  <div class="col-xs-6 col-sm-6 col-md-4 col-lg-4">
                    <div class="well">
                      <li class="article-title">
                        <%= link_to "#{category.name}", category_path(category) %>
                        <!-- <span class="badge category">324</span> -->
                      </li>
                    </div>
                  </div>
                <% end %>
              </div>
            </div>
          </ul>
        <div align="center">
      <%= will_paginate %></div>
    </div>
  </div>
</div>
